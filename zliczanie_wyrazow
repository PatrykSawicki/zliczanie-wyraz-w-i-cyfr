#include <iostream>
#include <string>
#include <cctype>

int main()
{
    std::string s1;
    int count1{}, count2{} ;
    while(std::getline(std::cin, s1)){
        for(unsigned int i = 0; i <= s1.size() ; i++)
        {
            if(isdigit(s1[i]) && (!isdigit(s1[i+1]))) count1++;
            if(isalpha(s1[i]) && (!isalpha(s1[i+1]))) count2++;
        }
        std::cout << count1 <<" "<< count2 << std::endl;
        count1 = 0; count2 = 0;
    }
    return 0;
}
